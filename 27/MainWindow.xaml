<Window x:Class="_27.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_27"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListView x:Name ="lwCar">
            <ListView.View>
                <GridView>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=Number}"
                                    Header="Номер"/>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=NameCar}"
                Header="название машины"/>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=DateService}"
                Header="дата"/>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=FIO}"
Header="ФИО"/>
                    <GridViewColumn DisplayMemberBinding="{Binding Path=Age}"
Header="возраст"/>

                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
